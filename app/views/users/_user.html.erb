<div id="<%= dom_id user %>">
  <p>
    <strong>First_Name:</strong>
    <%= user.first_name %>
  </p>

  <p>
    <strong>Last_Name:</strong>
    <%= user.last_name %>
  </p>

  <p>
    <strong>Age:</strong>
    <%= user.age %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Phone Number:</strong>
    <%= user.phone_number %>
  </p>

</div>
