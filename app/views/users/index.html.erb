<p style="color: green"><%= notice %></p>

<h1>Users</h1>

<div id="users">
  <table id="my-table">
    <thead>
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Age</th>
        <th>Email</th>
        <th>Phone Number</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.first_name %></td>
          <td><%= user.last_name %></td>
          <td><%= user.age %></td>
          <td><%= user.email %></td>
          <td><%= user.phone_number %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>


<%= link_to "New user", new_user_path %>
